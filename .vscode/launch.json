    "version": "0.2.0",
    "configurations": [
        {
            "name": "RUN LAUNCHER DEBUG",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/crates/target/debug/inox_launcher.exe",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopOnEntry": false,
            "sourceLanguages": [
                "rust"
            ],
            "requireExactSource": false,
            "postDebugTask": "CLEANUP",
            "sourceFileMap": {
                "/rustc/09c42c45858d5f3aedfa670698275303a3d19afa": "${env:USERPROFILE}/.rustup/toolchains/nightly-x86_64-pc-windows-msvc/lib/rustlib/src/rust"
            },
            "console": "integratedTerminal",
            "symbolSearchPath": "${workspaceFolder}/crates/target//debug/, https://msdl.microsoft.com/download/symbols",
        },
        {
            "name": "RUN EDITOR DEBUG",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/crates/target/debug/inox_launcher.exe",
            "args": [
                "-plugin inox_editor"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "requireExactSource": false,
            "postDebugTask": "CLEANUP",
            "sourceLanguages": [
                "rust"
            ],
            "sourceFileMap": {
                "/rustc/09c42c45858d5f3aedfa670698275303a3d19afa": "${env:USERPROFILE}/.rustup/toolchains/nightly-x86_64-pc-windows-msvc/lib/rustlib/src/rust"
            },
            "console": "integratedTerminal",
            "symbolSearchPath": "${workspaceFolder}/crates/target/debug/in_use/, ${workspaceFolder}/crates/target/debug/, https://msdl.microsoft.com/download/symbols",
        },
        {
            "name": "RUN VIEWER DEBUG",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/crates/target/debug/inox_launcher.exe",
            "args": [
                "-plugin inox_connector",
                "-plugin inox_common_script",
                "-plugin inox_viewer",
                //"-load_file",
                //"C:\\PROJECTS\\INOX\\data\\blender_export\\TestScene\\TestScene.scene"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "requireExactSource": false,
            "postDebugTask": "CLEANUP",
            "sourceLanguages": [
                "rust"
            ],
            "sourceFileMap": {
                "/rustc/09c42c45858d5f3aedfa670698275303a3d19afa": "${env:USERPROFILE}/.rustup/toolchains/nightly-x86_64-pc-windows-msvc/lib/rustlib/src/rust"
            },
            "console": "integratedTerminal",
            "symbolSearchPath": "${workspaceFolder}/crates/target/debug/in_use/, ${workspaceFolder}/crates/target/debug/, https://msdl.microsoft.com/download/symbols",
        },
        {
            "name": "RUN CONTENT BROWSER DEBUG",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/crates/target/debug/inox_launcher.exe",
            "args": [
                "-plugin inox_content_browser"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "requireExactSource": false,
            "postDebugTask": "CLEANUP",
            "sourceLanguages": [
                "rust"
            ],
            "sourceFileMap": {
                "/rustc/09c42c45858d5f3aedfa670698275303a3d19afa": "${env:USERPROFILE}/.rustup/toolchains/nightly-x86_64-pc-windows-msvc/lib/rustlib/src/rust"
            },
            "console": "integratedTerminal",
            "symbolSearchPath": "${workspaceFolder}/crates/target/debug/in_use/, ${workspaceFolder}/crates/target/debug/, https://msdl.microsoft.com/download/symbols",
        },
        {
            "name": "RUN LAUNCHER RELEASE",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/crates/target/release/inox_launcher.exe",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopOnEntry": false,
            "sourceLanguages": [
                "rust"
            ],
            "requireExactSource": false,
            "postDebugTask": "CLEANUP",
            "sourceFileMap": {
                "/rustc/09c42c45858d5f3aedfa670698275303a3d19afa": "${env:USERPROFILE}/.rustup/toolchains/nightly-x86_64-pc-windows-msvc/lib/rustlib/src/rust"
            },
            "console": "integratedTerminal",
            "symbolSearchPath": "${workspaceFolder}/crates/target/release/, https://msdl.microsoft.com/download/symbols",
        },
    ]
