            },
            "console": "integratedTerminal",
            "symbolSearchPath": "${workspaceFolder}/crates/target/debug/in_use/, ${workspaceFolder}/crates/target/debug/, https://msdl.microsoft.com/download/symbols",
        },
        {
            "name": "RUN LAUNCHER RELEASE",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/crates/target/release/inox_launcher.exe",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopOnEntry": false,
            "sourceLanguages": [
