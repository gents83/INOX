{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "RUN REMOTE ANDROID DEBUG",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "apk",
                    "run",
                    "-p android-build"
                ],
                "filter": {
                    "name": "android-build",
                    "kind": "lib"
                }
            },
            "initCommands": [
                "platform select remote-android", // Execute `platform list` for a list of available remote platform plugins.
                "platform connect connect://emulator-5554:5045",
                "settings set target.inherit-env false", // See the note below.
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "RUN LAUNCHER DEBUG",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/crates/target/debug/inox_launcher.exe",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopOnEntry": false,
            "sourceLanguages": [
                "rust"
            ],
            "requireExactSource": false,
            "postDebugTask": "CLEANUP",
            "sourceFileMap": {
                "/rustc/9d1b2106e23b1abd32fce1f17267604a5102f57a": "${env:USERPROFILE}/.rustup/toolchains/nightly-x86_64-pc-windows-msvc/lib/rustlib/src/rust"
            },
            "console": "integratedTerminal",
            "symbolSearchPath": "${workspaceFolder}/crates/target//debug/, https://msdl.microsoft.com/download/symbols",
        },
        {
            "name": "RUN VIEWER RELEASE",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/crates/target/release/inox_launcher.exe",
            "args": [
                "-plugin inox_binarizer",
                "-plugin inox_connector",
                "-plugin inox_common_script",
                "-plugin inox_viewer",
                "-load_file",
                "models/craftman/craftman.scene"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "requireExactSource": false,
            "postDebugTask": "CLEANUP",
            "sourceLanguages": [
                "rust"
            ],
            "sourceFileMap": {
                "/rustc/9d1b2106e23b1abd32fce1f17267604a5102f57a": "${env:USERPROFILE}/.rustup/toolchains/nightly-x86_64-pc-windows-msvc/lib/rustlib/src/rust"
            },
            "console": "integratedTerminal",
            "symbolSearchPath": "${workspaceFolder}/crates/target/debug/in_use/, ${workspaceFolder}/crates/target/debug/, https://msdl.microsoft.com/download/symbols",
        },
        {
            "name": "RUN VIEWER DEBUG",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/crates/target/debug/inox_launcher.exe",
            "args": [
                "-plugin inox_binarizer",
                "-plugin inox_connector",
                "-plugin inox_common_script",
                "-plugin inox_viewer",
                "-load_file",
                "models/craftman/craftman.scene"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "requireExactSource": false,
            "postDebugTask": "CLEANUP",
            "sourceLanguages": [
                "rust"
            ],
            "sourceFileMap": {
                "/rustc/9d1b2106e23b1abd32fce1f17267604a5102f57a": "${env:USERPROFILE}/.rustup/toolchains/nightly-x86_64-pc-windows-msvc/lib/rustlib/src/rust"
            },
            "console": "integratedTerminal",
            "symbolSearchPath": "${workspaceFolder}/crates/target/debug/in_use/, ${workspaceFolder}/crates/target/debug/, https://msdl.microsoft.com/download/symbols",
        },
        {
            "name": "RUN BINARIZER DEBUG",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/crates/target/debug/inox_launcher.exe",
            "args": [
                "-plugin inox_binarizer"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "requireExactSource": false,
            "postDebugTask": "CLEANUP",
